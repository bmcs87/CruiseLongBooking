####Long Term Cruise Booking
#Call the file
cruise <- read.csv("cruise.txt")

#Install Packages
install.packages("ggplot2")
install.packages("forecast")
install.packages("tseries")
library('ggplot2', 'forecast', 'tseries')

####SRPV - Daily
acf(cruise$DailySearch, title = "Daily")

####SRPV - Weekly
acf(cruise$WeeklySearch, title = "Weekly")

####SRPV - Monthly
acf(cruise$MonthSearch, title = "Monthly")
